<%# Include the header module (partial) %> 
<%- include('./partials/head.ejs') %> 

<div class="game">
    <header>
        <h1>Welcome</h1>
        <p>Connected as: <span class="username"><%= userName %></span></p>
    </header>
    <aside>
        <section>
            <h3>Scoreboard:</h3>
            <p>Round 1</p>
        </section>
    </aside>
    <main>
        <section class="messages">
            <h2>Messages:</h2>

        </section>
        <section class="form">
            <h2>Form:</h2>
            <form name="guessing">
                <input type="text" placeholder="Say hi to the other players!" />
                <input type="submit" value="Send">
            </form>
        </section>
    </main>
</div>

<%# Include the header module (partial) %> 
<%- include('./partials/footer.ejs') %> 