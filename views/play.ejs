<div class="game">
    <section class="end-scores">
        <h2>🏆</h2>
        <h2>Lars won the game!</h2>
        <p>Game ended. Here are the scores:</p>
        <ol>
            
        </ol>
        <a href="/">Back to start</a>
    </section>
    <aside>
        <section>
            <p>Round 1</p>
            <h3>Scoreboard</h3>
            <p>Connected as: <span class="username"><%= userName %></span></p>
        </section>
    </aside>
    <section class="messages">
    </section>
    <section class="hints">
        <div class="imagecontainer">
            <figure>
                <img src="/images/guessimage.png" alt="">
            </figure>
            <figure>
                <img src="/images/questionmarks.png" alt="">
            </figure>
        </div>
    </section>
    <section class="form">
        <form name="guessing">
            <input type="text" placeholder="Send a message..." />
            <input type="submit" value="Send">
        </form>
    </section>
</div>

