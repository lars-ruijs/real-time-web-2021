<%# Include the header module (partial) %> 
<%- include('./partials/head.ejs') %> 

<div class="game">
    <aside>
        <section>
            <p>Round 1</p>
            <h3>Scoreboard</h3>
            <p>Connected as: <span class="username"><%= userName %></span></p>
        </section>
    </aside>
    <section class="messages">
    </section>
    <section class="hints">
        <div class="imagecontainer">
            <figure>
                <img src="/images/guessimage.png" alt="">
            </figure>
            <figure>
                <img src="/images/questionmarks.png" alt="">
            </figure>
        </div>
    </section>
    <section class="form">
        <form name="guessing">
            <input type="text" placeholder="Say hi to the other players!" />
            <input type="submit" value="Send">
        </form>
    </section>
</div>

<%# Include the header module (partial) %> 
<%- include('./partials/footer.ejs') %> 